        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Action</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <% if (!userWishlist) { %>
                                            <h4>No items in the wishlist</h4>
                                        <% } else { %>
                                            <% userWishlist.products.forEach(product => { %>
                                                <tr> 
                                                    <td class="image product-thumbnail"><img src="<%= product.productId.photos[0] %>" alt="#"></td>
                                                    <td class="product-des product-name">
                                                        <h5 class="product-name"><%= product.productId.name %></h5>
                                                    </td>
                                                    <td class="price" data-title="Price"><span><%= product.productId.s_price %></span></td>
                                                 
                                                    <td class="text-right" data-title="Cart">
                                                        <a href="/wishlistTocart/<%= product.productId._id %>">
                                                            <button class="btn btn-sm"><i class="fi-rs-shopping-bag mr-5"></i>Add to cart</button>
                                                        </a>
                                                    </td>
                                                    <td class="action" data-title="Remove"><a href="javascript:void(0);" class="remove-item" data-product-id="<%= product.productId._id %>"><i class="fi-rs-trash"></i></a></td>
                                                </tr> 
                                            <% }); %>
                                        <% } %>
                                    </tr>
                                    
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>


    <script>

document.addEventListener("DOMContentLoaded", function () {
    const removeButtons = document.querySelectorAll(".remove-item");

    removeButtons.forEach(removeButton => {
      removeButton.addEventListener("click", (event) => {
        event.preventDefault();

        const productId = removeButton.getAttribute("data-product-id");
        console.log(productId);

        fetch(`/removefromwishlist/${productId}`, {
          method: "POST", 
          headers: {
            'Content-Type': 'application/json'
          }
          
        })
   
          .then(data => {
              window.location.href = "/wishlist";
          })
          .catch(error => {
            console.error("Error removing product:", error);
          });
      });
    });
  });
    </script>
