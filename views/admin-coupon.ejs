        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupon Management</h2>
                    <p>Manage the coupons</p>
                    <button  class="btn btn-primary btn-sm rounded"><a style="color: aliceblue;" href="/admin/addcoupon">Add coupon</a></button>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <% if (!couponlist) { %>
                            <h5>Currently there are no coupons to show...</h5>
                        <% } else { %>
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <!-- <th scope="col">Sl.no</th> -->
                                        <th scope="col">Code</th>
                                        <th scope="col">Coupon Text</th>
                                        <th scope="col">Discount Value</th>
                                        <th scope="col">Minimum Order Amount</th>
                                        <th scope="col">Expiration Date</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% couponlist.forEach((coupon) => { %>
                                        <tr>
                                            <!-- <td></td> -->
                                            <td><%= coupon.couponCode %></td>
                                            <td><%=coupon.couponType%></td>
                                            <td><%= coupon.amount %></td>
                                            <td><%= coupon.minimumPurchase %></td>
                                            <td><%= coupon.expiryDate.toLocaleDateString('en-US') %></td>
                                            <td><%= coupon.status %></td>
                                            <td class="">
                                                <a href="/admin/edit-coupon/<%= coupon._id %>" class="btn btn-md rounded font-sm">Edit</a>
                                                
                                                <% if (coupon.status =="Inactive") { %>
                                                    <a href="/admin/unblock-coupon/<%= coupon._id %>" class="btn btn-success">Unblock</a>
                                                <% } else { %>
                                                    <a href="/admin/block-coupon/<%= coupon._id %>" class="btn btn-danger">Block</a>
                                                <% } %>
                                            </td>
                                        </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                        <% } %>
                    </div>
                </div>
                
            </div>
             <!-- card end// -->

             



      
