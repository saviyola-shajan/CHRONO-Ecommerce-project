                <script>
                    const imageContainer = document.querySelector('.image-container');
                    const productImage = document.querySelector('.product-image');
                    const thumbnails = document.querySelectorAll('.thumbnail');

                    // Function to update the main image when a thumbnail is clicked
                    function updateMainImage(thumbnail) {
                        const dataImage = thumbnail.getAttribute('data-image');
                        productImage.src = dataImage;
                    }

                    // Add event listeners to thumbnails
                    thumbnails.forEach((thumbnail) => {
                        thumbnail.addEventListener('click', () => {
                            updateMainImage(thumbnail);
                        });
                    });

                    // Add zoom effect to the main image container
                    imageContainer.addEventListener('mousemove', (e) => {
                        // Calculate the position of the cursor within the image container
                        const x = (e.clientX - imageContainer.getBoundingClientRect().left) / imageContainer.offsetWidth;
                        const y = (e.clientY - imageContainer.getBoundingClientRect().top) / imageContainer.offsetHeight;

                        // Adjust the size of the main product image for zoom
                        productImage.style.transform = `scale(2) translate(-${x * 50}%, -${y * 50}%)`;
                    });

                    imageContainer.addEventListener('mouseleave', () => {
                        // Reset the main product image back to its original size when the cursor leaves the container
                        productImage.style.transform = 'scale(1) translate(0, 0)';
                    });

                </script>


