                                        button.addEventListener("click", function (e) {
                                            e.preventDefault();
                            
                                            const productId = button.getAttribute("data-product-id");
                            
                                            fetch(`/addtowishlist?productId=${productId}`, {
                                                method: "get",
                                                headers: {
                                                    "content-type": "application/json",
                                                },
                                            })
                                                .then((response) => {
                                                    if (!response.ok) {
                                                        throw new Error("Product already in the Wishlist");
                                                    }
                            
                                                    return response.json();
                                                })
                                                .then((data) => {
                                                    console.log("Added to wishlist:", data);
                            
                                                    Swal.fire({
                                                        position: 'top-centre',
                                                        icon: 'success',
                                                        title: 'Added to Wishlist',
                                                        showConfirmButton: false,
                                                        timer: 1500
                                                    });
                                                })
                                                .catch((error) => {
                                                    console.error(error);
                                                    Swal.fire({
                                                        icon: "error",
                                                        title: "Oops...",
                                                        text: error.message || "Failed to add product to Wishlist",
                                                        footer: '',
                                                        timer: 1500
                                                    });
                                                });
                                        });
                                    });
                                });
