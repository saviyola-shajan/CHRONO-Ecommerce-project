                    <script>
                        document.addEventListener("DOMContentLoaded", function () {
                            const checkboxes = document.querySelectorAll('input[type="checkbox"]');
                    
                            function loadCheckboxSelections() {
                                const savedValues = localStorage.getItem('checkboxSelections');
                    
                                if (savedValues) {
                                    const parsedValues = JSON.parse(savedValues);
                    
                                    checkboxes.forEach(function (checkbox) {
                                        if (parsedValues.hasOwnProperty(checkbox.value)) {
                                            checkbox.checked = parsedValues[checkbox.value];
                                        }
                                    });
                                }
                            }
                    
                            loadCheckboxSelections();
                    
                            const filterForm = document.getElementById('filterForm');
                            filterForm.addEventListener('submit', function () {
                                const selectedValues = {};
                    
                                checkboxes.forEach(function (checkbox) {
                                    selectedValues[checkbox.value] = checkbox.checked;
                                });
                    
                                localStorage.setItem('checkboxSelections', JSON.stringify(selectedValues));
                            });
                    
                            const clearButton = document.querySelector('.btn-default[href="/"]');
                            clearButton.addEventListener('click', function () {
                                localStorage.removeItem('checkboxSelections');
                                checkboxes.forEach(function (checkbox) {
                                    checkbox.checked = false;
                                });
                            });
                        });
                    </script>
                    
                    
