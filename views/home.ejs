                                  button.addEventListener("click", function (event) {
                                    event.preventDefault();
                              
                                    const productId = button.getAttribute("data-product-id");
                              
                                    fetch(`/addToCart?productId=${productId}`, {
                                      method: "get",
                                      headers: {
                                        "Content-Type": "application/json",
                                      },
                                    })
                                    .then((response) => response.json())
          .then((data) => {
            if (data.isProductAdded) {
              console.log("Added to cart:", data);
              Swal.fire({
                position: 'top-centre',
                icon: 'success',
                title: 'Added to cart',
                showConfirmButton: false,
                timer: 1500
              });
            } else {
              console.error("Failed to add the product:", data.error || "Unknown error");
              Swal.fire({
                icon: "error",
                title: "Oops...",
                text: data.error || "Failed to add the product to the cart",
                footer: '',
                timer: 1500
              });
            }
          })
          .catch((error) => {
            console.error("Error adding to cart:", error);
            Swal.fire({
              icon: "error",
              title: "Oops...",
              text: "Failed to add the product to the cart",
              footer: '',
              timer: 1500
            });
            window.location.href = "/get-login";
