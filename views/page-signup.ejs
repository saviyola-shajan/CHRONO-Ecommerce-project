    <script>
       document.addEventListener("DOMContentLoaded", function () {
    const registrationForm = document.getElementById("registrationForm");
    const otpInput = document.getElementById("otp");
    const sendOTPButton = document.getElementById("sendOTPButton");
    const verifyOTPButton = document.getElementById("verifyOTPButton");
    const signInButton = document.getElementById("signInButton");

    // Initially, disable the "Sign In" button
    signInButton.disabled = true;

    sendOTPButton.addEventListener("click", function (e) {
        e.preventDefault();

        const phoneNumber = document.querySelector('input[name="phoneNumber"]').value;

        if (!phoneNumber) {
            errSignupPhoneNumber.innerHTML = "Phone number is required.";
            return;
        }

        fetch(`/sent-otp?phoneNumber=${phoneNumber}`)
            .then((response) => {
                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }
                return response.json();
            })
            .then((data) => {
                console.log("Response from server:", data);

                Swal.fire({
                    position: 'top-centre',
                    icon: 'success',
                    title: 'OTP sent successfully',
                    showConfirmButton: false,
                    timer: 1500
                });
                startResendTimer();
                // Enable the "Sign In" button
                signInButton.disabled = false;
            })
            .catch((error) => {
                console.error("Error sending OTP:", error);
            });
    });

    

    function startResendTimer() {
        let remaningTime = 60;
        timerElement.style.display = "block";
        resendOTPButton.style.display = "none";

        const countdown = setInterval(() => {
            timerElement.textContent = `Resend in ${remaningTime} seconds`;
            remaningTime--;

            if (remaningTime < 0) {
                clearInterval(countdown);
                timerElement.style.display = "none";
                resendOTPButton.style.display = "block";
            }
        }, 1000);
    }

    verifyOTPButton.addEventListener("click", function (e) {
        e.preventDefault();

        const otp = otpInput.value;
        const phoneNumber = document.querySelector('input[name="phoneNumber"]').value;

        if (!phoneNumber || !otp) {
            errSignupOtp.innerHTML = "Phone number and OTP are required.";
            return;
        }

        fetch(`/verify-otp?phoneNumber=${phoneNumber}&otp=${otp}`, {
            method: "POST",
            body: otpInput.value
        })
            .then((response) => {
                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }
                return response.json();
            })
            .then((data) => {
                console.log("OTP verified:", data);
                Swal.fire({
                    position: 'top-centre',
                    icon: 'success',
                    title: 'OTP Verified',
                    showConfirmButton: false,
                    timer: 1500
                });
                // Enable the "Sign In" button
                signInButton.disabled = false;
            })
            .catch((error) => {
                console.error("Error verifying OTP:", error);
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'Incorrect OTP!',
                });
            });
    });
    });

        

            // const loginForm = document.querySelector('form[action="/signin"]');
            // const loginEmailInput = document.getElementById("loginEmail");
